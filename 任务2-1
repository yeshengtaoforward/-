void sort(int *a, int left, int right)
{
	if(left>=right)
		return;
	int key=a[left];
	int i=left;
	int j=right;
	while(i<j)
	{
		while((i<j)&&(a[j]>key))
		{
			j--;
		}
		a[i]=a[j];
		while((i<j)&&(a[i]<key))
		{
			i++;
		}
		a[j]=a[i];
	}
	    a[i]=key;
		sort(a,left,i-1);
		sort(a,i+1,right);
	
}
#include<stdio.h>
void main()
{
	int a[10]={1,3,4,5,7,6,8,9,2,10};
	sort(a,1,9);
for(int i=0;i<10;i++)
{
	printf("%2d",a[i]);
}
}
